DATABASE_URL={YOUR_POSTGRESQL}
OPENAI_API_KEY={YOUR_API_KEY}
NODE_ENV=development
NEXT_PUBLIC_BASE_PATH=/mockapi
LLM_SERVICE_URL=http://localhost:8000

# NextAuth 설정
# 중요: basePath를 사용하는 경우 NEXTAUTH_URL은 basePath를 포함한 전체 URL로 설정해야 합니다
# 예: NEXT_PUBLIC_BASE_PATH=/mockapi 인 경우
# NEXTAUTH_URL=http://localhost:3000/mockapi
# Google OAuth 콜백 URL도 다음으로 설정: http://localhost:3000/mockapi/api/auth/callback/google
NEXTAUTH_URL=http://localhost:3000/mockapi
NEXTAUTH_SECRET=your-secret-key-here-change-in-production

# Google OAuth 설정
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret